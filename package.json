    "jest": {
        "testEnvironment": "jsdom",
        "testEnvironmentOptions": {
            "url": "http://localhost/"
        },
        "testMatch": [
            "<rootDir>/test/**/*-test.[tj]s?(x)"
        ],
        "setupFiles": [
            "jest-canvas-mock"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/matrix-react-sdk/test/setupTests.js"
        ],
        "moduleNameMapper": {
            "\\.(css|scss|pcss)$": "<rootDir>/__mocks__/cssMock.js",
            "\\.(gif|png|ttf|woff2)$": "<rootDir>/src/matrix-react-sdk/__mocks__/imageMock.js",
            "\\.svg$": "<rootDir>/src/matrix-react-sdk/__mocks__/svg.js",
            "\\$webapp/i18n/languages.json": "<rootDir>/src/matrix-react-sdk/__mocks__/languages.json",
            "^react$": "<rootDir>/node_modules/react",
            "^react-dom$": "<rootDir>/node_modules/react-dom",
            "^matrix-js-sdk$": "<rootDir>/node_modules/matrix-js-sdk/src",
            "^matrix-react-sdk$": "<rootDir>/src/matrix-react-sdk/src",
            "decoderWorker\\.min\\.js": "<rootDir>/src/matrix-react-sdk/__mocks__/empty.js",
            "decoderWorker\\.min\\.wasm": "<rootDir>/src/matrix-react-sdk/__mocks__/empty.js",
            "waveWorker\\.min\\.js": "<rootDir>/src/matrix-react-sdk/__mocks__/empty.js",
            "context-filter-polyfill": "<rootDir>/src/matrix-react-sdk/__mocks__/empty.js",
            "FontManager.ts": "<rootDir>/src/matrix-react-sdk/__mocks__/FontManager.js",
            "workers/(.+)\\.worker\\.ts": "<rootDir>/src/matrix-react-sdk/__mocks__/workerMock.js",
            "^!!raw-loader!.*": "jest-raw-loader",
            "RecorderWorklet": "<rootDir>/src/matrix-react-sdk/__mocks__/empty.js"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!matrix-js-sdk).+$"
        ],
        "coverageReporters": [
            "text-summary",
            "lcov"
        ],
        "testResultsProcessor": "@casualbot/jest-sonar-reporter"
    },
    "@casualbot/jest-sonar-reporter": {
        "outputDirectory": "coverage",
        "outputName": "jest-sonar-report.xml",
        "relativePaths": true
    }
